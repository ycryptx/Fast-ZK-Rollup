{
  "name": "@ycryptx/sequencer",
  "version": "1.0.0",
  "description": "A fast sequencer written for Mina rollups",
  "type": "commonjs",
  "main": "bundle/src/index.js",
  "types": "build/src/index.d.ts",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.23.1",
    "@types/jest": "~29.5",
    "@types/node": "~18",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "~5.59",
    "@typescript-eslint/parser": "~5.59",
    "eslint": "~8.38",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "~27.2",
    "eslint-plugin-prettier": "^4.2.1",
    "grpc-tools": "^1.12.4",
    "jest": "~29.5",
    "prettier": "~2.8",
    "rimraf": "^5.0.5",
    "ts-api-utils": "~0.0.44",
    "ts-jest": "~29.1",
    "ts-proto": "^1.150.1",
    "typescript": "^5.0.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "node bundle/bundle.js",
    "clean": "rimraf coverage build bundle",
    "compile:zkapp": "tsc -p tsconfig.json && webpack --config webpack.config.compiler.js && node ./bundle/compiler.js",
    "prebuild": "npm run lint",
    "build": "npm run clean && npm run build:protobuf && tsc -p tsconfig.json && webpack",
    "build:scripts": "tsc -p tsconfig.json && webpack --config webpack.config.mapper.js && webpack --config webpack.config.reducer.js",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:protobuf": "cd ./proto && ../node_modules/.bin/buf lint; cd ..; ./node_modules/.bin/grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/server/compiled --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false --proto_path=./proto ./proto/services/sequencer/v1/sequencer_service.proto --descriptor_set_out=./proto/protoset.bin --include_imports",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "author": "@ycryptx",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-emr": "^3.28.0",
    "@aws-sdk/client-s3": "^3.28.0",
    "@aws-sdk/lib-storage": "^3.28.0",
    "@ycryptx/rollup": "1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "long": "^5.2.3",
    "nice-grpc": "^2.1.4",
    "nice-grpc-server-reflection": "^2.0.6",
    "o1js": "^0.14.1",
    "protobufjs": "^7.2.4",
    "randomstring": "^1.3.0",
    "source-map-support": "^0.5.21",
    "tslib": "~2.5",
    "winston": "^3.11.0"
  }
}
