{
  "name": "@ycryptx/map-reduce-scripts",
  "version": "1.0.0",
  "description": "Map and reduce scripts to be run by Hadoop Streaming",
  "type": "commonjs",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "devDependencies": {
    "@types/jest": "~29.5",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~5.59",
    "@typescript-eslint/parser": "~5.59",
    "eslint": "~8.38",
    "eslint-config-prettier": "~8.8",
    "eslint-plugin-jest": "~27.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "~29.5",
    "jest-mock-process": "^2.0.0",
    "mock-stdin": "^1.0.0",
    "prettier": "~2.8",
    "rimraf": "~5.0",
    "ts-api-utils": "~0.0.44",
    "ts-jest": "~29.1",
    "typescript": "~4.7.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf build && rimraf bundle",
    "prebuild": "npm run lint",
    "build": "npm run clean && tsc -p tsconfig.json && webpack --config webpack.config.mapper.js && webpack --config webpack.config.reducer.js",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "author": "@ycryptx",
  "license": "Apache-2.0",
  "dependencies": {
    "@ycryptx/rollup": "1.0.0",
    "cross-env": "^7.0.3",
    "o1js": "^0.13.1",
    "tslib": "~2.5"
  }
}
